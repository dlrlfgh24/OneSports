<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.familytoto.familytotoProject.mybatis.webLog.mapper">


	<insert id="insertWebLog">
		INSERT INTO WEB_LOG (CURRENT_URL, PREV_URL, IP, CUST_NO)
		VALUES(#{currentUrl},#{prevUrl},#{ip},#{custNo})
	</insert>
